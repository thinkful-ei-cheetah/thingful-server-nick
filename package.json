    "eslint-config-node": "^4.0.0",
    "eslint-plugin-mocha-only": "0.0.3",
    "eslint-plugin-node": "^9.0.1",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "pg": "^7.8.0",
    "treeize": "^2.1.2",
